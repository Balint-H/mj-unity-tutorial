        m_UnmaskedViews: []
      m_Summary: 
      m_Description: 
      m_InstructionBoxTitle: 
      m_InstructionText: 
      m_TutorialButtonText: 
    - m_Type: 1
      Title:
        m_Untranslated: Import the MJCF file
      Text:
        m_Untranslated: "Navigate to and select in the File Menu Toolbar: \n\n<b>Assets
          > \"Import MuJoCo Scene\"</b>\n\nIt is near the bottom of the dropdown
          menu. Then, select  <b>cartpole.xml</b> in the dialogue, located in the
          following folder:\n\n<b>Tutorials > 2 - Scene Building > MJCF</b> \n\nClick
          on \"Next\" below."
      m_Tutorial: {fileID: 0}
